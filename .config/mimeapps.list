[Default Applications]

# These .desktop entries can be seen in /usr/share/applications/.
# Custom .desktop entries can be added in ~/.local/share/applications/.

# xdg-open will use these settings to determine how to open filetypes.
# xdg-mime query filetype <FILE>      // Prints the MIME type of the file <FILE>.
# xdg-mime query default <MIME-TYPE>  // Prints the .desktop filename of the application
#                                     // which is registered to open <MIME-TYPE>.

image/jpeg=imv.desktop;
image/jpg=imv.desktop;
image/png=imv.desktop;
application/pdf=org.pwmt.zathura-pdf-mupdf.desktop;
x-scheme-handler/msteams=teams-for-linux.desktop
